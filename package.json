{
  "name": "ngrt-performance",
  "version": "1.2.10",
  "description": "Initial Commit",
  "main": "gulpfile.js",
  "scripts": {
    "postinstall": "npm run build",
    "start": "nodemon ./src/server/server.js",
    "build": "npm run vue-build && npm run vanilla-build && npm run angular-build && npm run dashboard-build && npm run react-build && npm run angular2-build && npm run server-build",
    "server": "gulp",
    "server-build": "gulp server-build",
    "vue": "rimraf src/client-vue/dist       && webpack --config _build/client-vue/webpack.conf.js --progress --colors --watch",
    "vue-build": "rimraf src/client-vue/dist       && webpack --config _build/client-vue/webpack.conf.js --progress --colors",
    "vanilla": "rimraf src/client-js/dist        && webpack --config _build/client-js/webpack.base.conf.js --progress --colors --watch",
    "vanilla-build": "rimraf src/client-js/dist        && webpack --config _build/client-js/webpack.base.conf.js --progress --colors",
    "angular": "rimraf src/client-ng1/dist       && webpack --config _build/client-ng1/webpack.base.conf.js --progress --colors --watch",
    "angular-build": "rimraf src/client-ng1/dist       && webpack --config _build/client-ng1/webpack.base.conf.js --progress --colors",
    "dashboard": "rimraf src/client-dashboard/dist && webpack --config _build/client-dashboard/webpack.conf.js --progress --colors --watch",
    "dashboard-build": "rimraf src/client-dashboard/dist && webpack --config _build/client-dashboard/webpack.conf.js --progress --colors",
    "react": "rimraf src/client-react/dist     && webpack --config _build/client-react/webpack.conf.js --progress --colors --watch",
    "react-build": "rimraf src/client-react/dist     && webpack --config _build/client-react/webpack.conf.js --progress --colors",
    "angular2": "rimraf src/client-ng/dist        && webpack --config _build/client-ng/webpack.conf.js --progress --colors --watch",
    "angular2-build": "rimraf src/client-ng/dist        && webpack --config _build/client-ng/webpack.conf.js --progress --colors",
    "major": "rimraf src/client-dashboard/dist && webpack --config _build/client-dashboard/webpack.conf.js --progress --colors --watch --major",
    "minor": "rimraf src/client-dashboard/dist && webpack --config _build/client-dashboard/webpack.conf.js --progress --colors --watch --minor",
    "patch": "rimraf src/client-dashboard/dist && webpack --config _build/client-dashboard/webpack.conf.js --progress --colors --watch --patch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BTplc/ngrt-performance.git"
